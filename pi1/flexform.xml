<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<sDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:ods_osm/locallang_db.xml:tt_content.pi_flexform.sheet_sDEF</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<marker>
						<TCEforms>
							<label>LLL:EXT:ods_osm/locallang_db.xml:tt_content.pi_flexform.marker</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages,fe_users,fe_groups,tt_address,tt_address_group,tx_odsosm_track,tx_odsosm_vector</allowed>
								<size>5</size>
								<minitems>0</minitems>
								<maxitems>-1</maxitems>
								<autoSizeMax>20</autoSizeMax>
								<show_thumbs>1</show_thumbs>
							</config>
						</TCEforms>
					</marker>
					<width>
						<TCEforms>
							<label>LLL:EXT:ods_osm/locallang_db.xml:tt_content.pi_flexform.width</label>
							<config>
								<type>input</type>
								<size>5</size>
							</config>
						</TCEforms>
					</width>
					<height>
						<TCEforms>
							<label>LLL:EXT:ods_osm/locallang_db.xml:tt_content.pi_flexform.height</label>
							<config>
								<type>input</type>
								<size>5</size>
							</config>
						</TCEforms>
					</height>
					<lon>
						<TCEforms>
							<label>LLL:EXT:ods_osm/locallang_db.xml:tx_odsosm_lon</label>
							<displayCond>FIELD:static:REQ:false</displayCond>
							<config>
								<type>input</type>
								<size>11</size>
								<checkbox>0.000000</checkbox>
								<default>0.000000</default>
								<eval>tx_lonlat</eval>
								<wizards type="array">
									<_PADDING>2</_PADDING>
									<searchcoordinates>
										<type>popup</type>
										<title>Search coordinates</title>
										<script>EXT:ods_osm/wizard/index.php</script>
										<icon>EXT:ods_osm/wizard/osm.png</icon>
										<JSopenParams>,width=600,height=400,status=0,menubar=0,scrollbars=0</JSopenParams>
									</searchcoordinates>
								</wizards>
							</config>
						</TCEforms>
					</lon>
					<lat>
						<TCEforms>
							<label>LLL:EXT:ods_osm/locallang_db.xml:tx_odsosm_lat</label>
							<displayCond>FIELD:static:REQ:false</displayCond>
							<config>
								<type>input</type>
								<size>10</size>
								<eval>tx_lonlat</eval>
							</config>
						</TCEforms>
					</lat>
					<zoom>
						<TCEforms>
							<label>LLL:EXT:ods_osm/locallang_db.xml:tt_content.pi_flexform.zoom</label>
							<config>
								<type>select</type>
								<size>1</size>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0"></numIndex>
										<numIndex index="1"></numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">1</numIndex>
										<numIndex index="1">1</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">2</numIndex>
										<numIndex index="1">2</numIndex>
									</numIndex>
									<numIndex index="3" type="array">
										<numIndex index="0">3</numIndex>
										<numIndex index="1">3</numIndex>
									</numIndex>
									<numIndex index="4" type="array">
										<numIndex index="0">4</numIndex>
										<numIndex index="1">4</numIndex>
									</numIndex>
									<numIndex index="5" type="array">
										<numIndex index="0">5</numIndex>
										<numIndex index="1">5</numIndex>
									</numIndex>
									<numIndex index="6" type="array">
										<numIndex index="0">6</numIndex>
										<numIndex index="1">6</numIndex>
									</numIndex>
									<numIndex index="7" type="array">
										<numIndex index="0">7</numIndex>
										<numIndex index="1">7</numIndex>
									</numIndex>
									<numIndex index="8" type="array">
										<numIndex index="0">8</numIndex>
										<numIndex index="1">8</numIndex>
									</numIndex>
									<numIndex index="9" type="array">
										<numIndex index="0">9</numIndex>
										<numIndex index="1">9</numIndex>
									</numIndex>
									<numIndex index="10" type="array">
										<numIndex index="0">10</numIndex>
										<numIndex index="1">10</numIndex>
									</numIndex>
									<numIndex index="11" type="array">
										<numIndex index="0">11</numIndex>
										<numIndex index="1">11</numIndex>
									</numIndex>
									<numIndex index="12" type="array">
										<numIndex index="0">12</numIndex>
										<numIndex index="1">12</numIndex>
									</numIndex>
									<numIndex index="13" type="array">
										<numIndex index="0">13</numIndex>
										<numIndex index="1">13</numIndex>
									</numIndex>
									<numIndex index="14" type="array">
										<numIndex index="0">14</numIndex>
										<numIndex index="1">14</numIndex>
									</numIndex>
									<numIndex index="15" type="array">
										<numIndex index="0">15</numIndex>
										<numIndex index="1">15</numIndex>
									</numIndex>
									<numIndex index="16" type="array">
										<numIndex index="0">16</numIndex>
										<numIndex index="1">16</numIndex>
									</numIndex>
									<numIndex index="17" type="array">
										<numIndex index="0">17</numIndex>
										<numIndex index="1">17</numIndex>
									</numIndex>
									<numIndex index="18" type="array">
										<numIndex index="0">18</numIndex>
										<numIndex index="1">18</numIndex>
									</numIndex>
								</items>   
							</config>
						</TCEforms>
					</zoom>
					<library>
						<TCEforms>
							<label>LLL:EXT:ods_osm/locallang_db.xml:tt_content.pi_flexform.library</label>
							<onChange>reload</onChange>
							<config>
								<type>select</type>
								<size>1</size>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0"></numIndex>
										<numIndex index="1"></numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">Leaflet</numIndex>
										<numIndex index="1">leaflet</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">Openlayers</numIndex>
										<numIndex index="1">openlayers</numIndex>
									</numIndex>
									<numIndex index="3" type="array">
										<numIndex index="0">Static</numIndex>
										<numIndex index="1">static</numIndex>
									</numIndex>
								</items>
							</config>
						</TCEforms>
					</library>
					<layer>
						<TCEforms>
							<label>LLL:EXT:ods_osm/locallang_db.xml:tt_content.pi_flexform.layer</label>
							<displayCond>FIELD:library:=:</displayCond>
							<config>
								<type>select</type>
								<size>3</size>
								<minitems>0</minitems>
								<maxitems>99</maxitems>
								<autoSizeMax>20</autoSizeMax>
								<foreign_table>tx_odsosm_layer</foreign_table>
								<foreign_table_where>AND tx_odsosm_layer.hidden=0 ORDER BY tx_odsosm_layer.sorting</foreign_table_where>
							</config>
						</TCEforms>
					</layer>
					<leaflet_layer>
						<TCEforms>
							<label>LLL:EXT:ods_osm/locallang_db.xml:tt_content.pi_flexform.layer</label>
							<displayCond>FIELD:library:=:leaflet</displayCond>
							<config>
								<type>select</type>
								<size>3</size>
								<minitems>0</minitems>
								<maxitems>99</maxitems>
								<autoSizeMax>20</autoSizeMax>
								<foreign_table>tx_odsosm_layer</foreign_table>
								<foreign_table_where>AND tx_odsosm_layer.hidden=0 AND tx_odsosm_layer.tile_url!="" ORDER BY tx_odsosm_layer.sorting</foreign_table_where>
							</config>
						</TCEforms>
					</leaflet_layer>
					<openlayers_layer>
						<TCEforms>
							<label>LLL:EXT:ods_osm/locallang_db.xml:tt_content.pi_flexform.layer</label>
							<displayCond>FIELD:library:=:openlayers</displayCond>
							<config>
								<type>select</type>
								<size>3</size>
								<minitems>0</minitems>
								<maxitems>99</maxitems>
								<autoSizeMax>20</autoSizeMax>
								<foreign_table>tx_odsosm_layer</foreign_table>
								<foreign_table_where>AND tx_odsosm_layer.hidden=0 AND tx_odsosm_layer.javascript!="" ORDER BY tx_odsosm_layer.sorting</foreign_table_where>
							</config>
						</TCEforms>
					</openlayers_layer>
					<static_layer>
						<TCEforms>
							<label>LLL:EXT:ods_osm/locallang_db.xml:tt_content.pi_flexform.layer</label>
							<displayCond>FIELD:library:=:static</displayCond>
							<config>
								<type>select</type>
								<size>1</size>
								<foreign_table>tx_odsosm_layer</foreign_table>
								<foreign_table_where>AND tx_odsosm_layer.hidden=0 AND tx_odsosm_layer.static_url!="" ORDER BY tx_odsosm_layer.sorting</foreign_table_where>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0"></numIndex>
										<numIndex index="1"></numIndex>
									</numIndex>
								</items>
							</config>
						</TCEforms>
					</static_layer>
					<mouse_navigation>
						<TCEforms>
							<label>LLL:EXT:ods_osm/locallang_db.xml:tt_content.pi_flexform.mouse_navigation</label>
							<displayCond>FIELD:library:=:openlayers</displayCond>
							<config>
								<type>check</type>
							</config>
						</TCEforms>
					</mouse_navigation>
					<show_pan_zoom>
						<TCEforms>
							<label>LLL:EXT:ods_osm/locallang_db.xml:tt_content.pi_flexform.show_pan_zoom</label>
							<displayCond>FIELD:library:=:openlayers</displayCond>
							<config>
								<type>select</type>
								<size>1</size>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0"></numIndex>
										<numIndex index="1"></numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:ods_osm/locallang_db.xml:tt_content.pi_flexform.show_pan_zoom.I.1</numIndex>
										<numIndex index="1">1</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">LLL:EXT:ods_osm/locallang_db.xml:tt_content.pi_flexform.show_pan_zoom.I.2</numIndex>
										<numIndex index="1">2</numIndex>
									</numIndex>
								</items>
							</config>
						</TCEforms>
					</show_pan_zoom>
					<show_popups>
						<TCEforms>
							<label>LLL:EXT:ods_osm/locallang_db.xml:tt_content.pi_flexform.show_popups</label>
							<displayCond>FIELD:library:IN:openlayers,leaflet</displayCond>
							<config>
								<type>select</type>
								<size>1</size>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0"></numIndex>
										<numIndex index="1"></numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:ods_osm/locallang_db.xml:tt_content.pi_flexform.show_popups.I.1</numIndex>
										<numIndex index="1">1</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">LLL:EXT:ods_osm/locallang_db.xml:tt_content.pi_flexform.show_popups.I.2</numIndex>
										<numIndex index="1">2</numIndex>
									</numIndex>
								</items>
							</config>
						</TCEforms>
					</show_popups>
					<marker_popup_initial>
						<TCEforms>
							<label>LLL:EXT:ods_osm/locallang_db.xml:tt_content.pi_flexform.marker_popup_intial</label>
							<displayCond>FIELD:show_popups:=:1</displayCond>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>fe_users,tt_address</allowed>
								<size>1</size>
								<minitems>0</minitems>
								<maxitems>1</maxitems>
								<show_thumbs>1</show_thumbs>
							</config>
						</TCEforms>
					</marker_popup_initial>
					<show_layerswitcher>
						<TCEforms>
							<label>LLL:EXT:ods_osm/locallang_db.xml:tt_content.pi_flexform.show_layerswitcher</label>
							<displayCond>FIELD:library:=:leaflet,openlayers</displayCond>
							<config>
								<type>check</type>
							</config>
						</TCEforms>
					</show_layerswitcher>
					<show_scalebar>
						<TCEforms>
							<label>LLL:EXT:ods_osm/locallang_db.xml:tt_content.pi_flexform.show_scalebar</label>
							<displayCond>FIELD:library:=:openlayers</displayCond>
							<config>
								<type>check</type>
							</config>
						</TCEforms>
					</show_scalebar>
				</el>
			</ROOT>
		</sDEF>
	</sheets>
</T3DataStructure>
